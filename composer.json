{
  "name": "geithware/debug-mysql-db",
  "type": "typo3-cms-extension",
  "description": "Extends \\TYPO3\\CMS\\Typo3DbLegacy\\Database (former \\TYPO3\\CMS\\Core\\Database\\DatabaseConnection and t3lib_db) to show Errors and Debug-Messages. Useful for viewing and debugging of sql-queries.",
  "keywords": [
    "TYPO3",
    "debug",
    "mysql",
    "query"
  ],
  "support": {
    "issues": "https://github.com/franzholz/debug_mysql_db/issues"
  },
  "homepage": "https://jambage.com",
  "authors": [
    {
      "name": "Franz Holzinger",
      "role": "Developer",
      "homepage": "https://ttproducts.de"
    }
  ],
  "license": [
    "GPL-2.0+"
  ],
  "require": {
    "typo3/cms-core": ">=9.0.0,<9.99.99",
    "friendsoftypo3/typo3db-legacy": ">=1.0.0,<1.2.0"
  },
  "autoload": {
    "psr-4": {
      "Geithware\\DebugMysqlDb\\": "Classes"
    }
  },
  "replace": {
    "geithware/debug_mysql_db": "self.version",
    "typo3-ter/debug-mysql-db": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "debug_mysql_db"
    }
  }
}
